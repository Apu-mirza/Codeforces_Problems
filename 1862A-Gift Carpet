#include<iostream>
using namespace std;

int main()
{
    int t, i, j, k, kicu;
    cin>>t;
    while(t--){
        long long n, m, i, jk;
        cin>>n>>m;
        int j, k=0;
        string s[n+3], ss="vika#";
        for(i=0; i<n; i++){
            cin>>s[i];
        }
        for(i=0; i<m; i++){
            for(j=0; j<n; j++){
                if(s[j][i] == ss[k]){
                    k++;
                    break;
                }
            }
        }
        if(k == 4){
            cout<<"Yes"<<endl;
        }
        else cout<<"No"<<endl;
    }
}

