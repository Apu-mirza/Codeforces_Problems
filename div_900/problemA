#include<iostream>

using namespace std;

int main()
{
    int t, n, k, i, j;
    int flag = 0;
    cin>>t;
    while(t--){
        cin>>n>>k;
        int array[n];

        for(i=0; i<n; i++){
            cin>>array[i];
        }

        for(i=0; i<n; i++){
            if(k == array[i]){
                flag = 1;
                break;
            }
            else flag = 0;
        }

        if(flag){
            cout<<"YES"<<endl;
        }else cout<<"NO"<<endl;
    }
}
